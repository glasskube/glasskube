{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://glasskube.dev/schemas/v1/versions.json",
  "$defs": {
    "PackageIndexItem": {
      "properties": {
        "version": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "version"
      ]
    }
  },
  "properties": {
    "versions": {
      "items": {
        "$ref": "#/$defs/PackageIndexItem"
      },
      "type": "array"
    },
    "latestVersion": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "type": "object",
  "required": [
    "versions",
    "latestVersion"
  ]
}
